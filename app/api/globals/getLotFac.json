        "sql": {
          "query": "select distinct \"tsptest\".\"lot_facturation\".num_lot_fac, \"tsptest\".\"lot_facturation\".libelle from \"tsptest\".\"lot_facturation\"  inner join \"tsptest\".\"suivi_lot_fac\" on \"tsptest\".\"lot_facturation\".\"num_lot_fac\" = \"tsptest\".\"suivi_lot_fac\".\"num_lot_fac\" where \"tsptest\".\"suivi_lot_fac\".\"id_periode\" = ? ",
          "params": [
            {
              "name": ":P1",
              "value": "{{$_GET.idperiod}}",
              "test": "489"
            }
          ]
        }
      },
      "output": true,
      "meta": [
