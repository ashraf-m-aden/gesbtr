  },
  "exec": {
    "steps": [
      {
        "name": "maxLogId",
        "module": "dbupdater",
        "action": "custom",
        "options": {
          "connection": "edbdev",
          "sql": {
            "query": "select (MAX(id_log)+1) as max from log_releve_correction",
            "params": []
          }
        },
        "output": true,
        "meta": [
          {
            "name": "max",
            "type": "number"
          }
        ],
        "type": "dbcustom_query"
      },
      {
        "name": "updateReleve",
        "module": "dbupdater",
        "action": "custom",
        "options": {
          "connection": "edbdev",
          "sql": {
            "query": "update releve set index_a=:p1,anomalie='VC' where num_releve=:p2",
            "params": [
              {
                "name": ":p1",
                "value": "{{$_GET.index_a}}"
              },
              {
                "name": ":p2",
                "value": "{{$_GET.num_releve}}"
              }
            ]
          }
        },
        "output": true,
        "meta": [],
        "outputType": "array",
        "type": "dbcustom_query"
      },
      {
        "name": "updateLog",
        "module": "dbupdater",
        "action": "custom",
        "options": {
          "connection": "edbdev",
          "sql": {
            "query": "insert into log_releve_correction (\"anomalie\", \"id_log\", \"index_ancien\", \"num_releve\", \"user_crea\") values (?, ?, ?, ?, ?) returning id_log",
            "params": [
              {
                "name": "?",
                "value": "{{$_GET.anomalie}}"
              },
              {
                "name": "?",
                "value": "{{maxLogId[0].max}}"
              },
              {
                "name": "?",
                "value": "{{$_GET.index_ancien}}"
              },
              {
                "name": "?",
                "value": "{{$_GET.num_releve}}"
              },
              {
                "name": "?",
                "value": "{{$_GET.user_crea}}"
              }
            ]
          }
        },
        "output": true,
        "meta": [],
        "outputType": "array",
        "type": "dbcustom_query"
      }
    ]
  }
}