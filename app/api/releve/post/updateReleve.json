      },
      {
        "type": "text",
        "name": "user_crea"
      },
      {
        "type": "text",
        "name": "num_releve"
      },
      {
        "type": "text",
        "name": "index_a"
      },
      {
        "type": "text",
        "name": "index_ancien"
      },
